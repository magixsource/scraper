{
  "_id": "ale",
  "startUrl": [
    "https://www.alebikewear.com/catalogue/men/apparel/jerseys/"
  ],
  "selectors": [
    {
      "id": "priceInStr",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": ".sheet-price span",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": ".product__main-image img",
      "multiple": false
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": ".text-image__text-block__description",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "items",
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "selector": "ul.listing__grid article.listing-block a",
      "multiple": true,
      "linkType": "linkFromHref"
    }
  ]
}