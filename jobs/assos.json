{
  "_id": "assos",
  "startUrl": [
    "https://www.assos.com/cn/man/clothing/jerseys.html"
  ],
  "selectors": [
    {
      "id": "page",
      "parentSelectors": [
        "_root",
        "page"
      ],
      "paginationType": "auto",
      "type": "SelectorPagination",
      "selector": ".jss104 div:nth-of-type(1) button.MuiButton-root"
    },
    {
      "id": "items",
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "selector": "div.AdvancedGrid-item>div>div>a",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "summary",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div.MuiTypography-body1",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "priceInStr",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div.price",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": "div[id=gridMainSlider]>div:nth-of-type(2) img",
      "multiple": false
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div.jss1193",
      "multiple": false,
      "regex": ""
    }
  ]
}