{
  "_id": "gaint-jersey",
  "startUrl": [
    "https://www.giant.com.cn/index.php/index/gear_finder.html?typeid=16"
  ],
  "selectors": [
    {
      "id": "items",
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "selector": "div.list_geartype>div.item_geartype:nth-of-type(1) .list_gear a",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "span.title",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "price",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "span.price",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorHTML",
      "selector": "div.imagetext_box",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "image",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorElementAttribute",
      "selector": ".cover div.swiper-slide-active",
      "multiple": false,
      "extractAttribute": "style",
      "regex": "https?://[^\\s>\"\\']+?\\.(?:jpe?g|png|gif|webp)(?:\\?[^\\s>\"\\']*)?"
    }
  ]
}