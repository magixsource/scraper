{
  "_id": "sobike",
  "startUrl": [
    "https://sobike-active.com/collections/man-road-cycling-ss-jersey"
  ],
  "selectors": [
    {
      "id": "items",
      "parentSelectors": [
        "_root",
        "pages"
      ],
      "type": "SelectorLink",
      "selector": "a.product-card__link--full-opacity",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "pages",
      "parentSelectors": [
        "_root",
        "pages"
      ],
      "paginationType": "auto",
      "type": "SelectorPagination",
      "selector": ".page a"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": ".slick-current img",
      "multiple": false
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div.product-single__box__block--description",
      "multiple": false,
      "regex": ""
    }
  ]
}