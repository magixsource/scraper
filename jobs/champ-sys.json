{
  "_id": "champ-sys",
  "startUrl": [
    "https://champ-sys.com/collections/jersey-1"
  ],
  "selectors": [
    {
      "id": "items",
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "selector": ".collection__products .product__grid__info a",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1.product__title",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "priceInStr",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "span.product__price",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": ".is-selected img.srcset",
      "multiple": false
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div[data-truncated-content]",
      "multiple": false,
      "regex": ""
    }
  ]
}