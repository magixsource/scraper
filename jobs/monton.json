{
  "_id": "monton",
  "startUrl": [
    "https://monton.cc/collections/men"
  ],
  "selectors": [
    {
      "id": "items",
      "parentSelectors": [
        "_root",
        "pages"
      ],
      "type": "SelectorLink",
      "selector": "a.hover-change-img",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "pages",
      "parentSelectors": [
        "_root",
        "pages"
      ],
      "paginationType": "auto",
      "type": "SelectorPagination",
      "selector": "a.pagination__item"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "priceInStr",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": ".price--large .price__last span.price-item--regular",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": "div.product-single__media-wrapper img:nth-of-type(1)",
      "multiple": false
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div.product-data-items-content",
      "multiple": false,
      "regex": ""
    }
  ]
}