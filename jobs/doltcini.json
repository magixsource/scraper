{
  "_id": "doltcini",
  "startUrl": [
    "https://e-doltcini.com/en/147-jersey-short-sleeves"
  ],
  "selectors": [
    {
      "id": "pages",
      "parentSelectors": [
        "_root",
        "pages"
      ],
      "paginationType": "auto",
      "type": "SelectorPagination",
      "selector": "a.next"
    },
    {
      "id": "items",
      "parentSelectors": [
        "_root",
        "pages"
      ],
      "type": "SelectorLink",
      "selector": ".product-title a",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "priceInStr",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "span.current-price-value",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": "img.js-qv-product-cover",
      "multiple": false
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div.product-description",
      "multiple": false,
      "regex": ""
    }
  ]
}