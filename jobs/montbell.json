{
  "_id": "montbell",
  "startUrl": [
    "https://www.montbell.com/us/en/products/list?c=28&f=2%3A132"
  ],
  "selectors": [
    {
      "id": "items",
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "selector": "a.products-item-name",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "priceInStr",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": ".price-container-shipping-from-1 .x-large span",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": ".visual-container img.product-name",
      "multiple": false,
      "extraPrepend": "https://www.montbell.com"
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "span.product-description",
      "multiple": false,
      "regex": ""
    }
  ]
}