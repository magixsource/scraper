{
  "_id": "trek",
  "startUrl": [
    "https://www.trekbikes.com/cn/zh_CN/%E8%87%AA%E8%A1%8C%E8%BD%A6%E6%9C%8D%E8%A3%85/%E9%AA%91%E8%A1%8C%E6%9C%8D/c/A206/"
  ],
  "selectors": [
    {
      "id": "page",
      "parentSelectors": [
        "_root",
        "page"
      ],
      "paginationType": "auto",
      "type": "SelectorPagination",
      "selector": "a.pagination__link"
    },
    {
      "id": "items",
      "parentSelectors": [
        "_root"
      ],
      "type": "SelectorLink",
      "selector": "li .cell a",
      "multiple": true,
      "linkType": "linkFromHref"
    },
    {
      "id": "title",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "h1.buying-zone__title",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "priceInStr",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "span.actual-price",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "brand_no",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": "div.buying-zone__code",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "summary",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorText",
      "selector": ".buying-zone__positioning-statement p",
      "multiple": false,
      "regex": ""
    },
    {
      "id": "coverPhoto",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorImage",
      "selector": "swiper-container img",
      "multiple": false
    },
    {
      "id": "content",
      "parentSelectors": [
        "items"
      ],
      "type": "SelectorHTML",
      "selector": "section#aftermarketProductFeaturesComponent",
      "multiple": false,
      "regex": ""
    }
  ]
}